{
  // 组件自动引入规则
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^Sh(.*)": "@/components/Sh$1.vue"
    }
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "black",
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/customers/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "black",
        "navigationBarTitleText": "获客",
        "disableScroll": true,
        "usingComponents": {
          "card-poster": "/pages/customers/components/CardPoster"
        }
      }
    },
    {
      "path": "pages/house/index",
      "style": {
        "navigationBarTitleText": "卖房",
        "navigationBarTextStyle": "black",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/consult/index",
      "style": {
        "navigationBarTitleText": "资讯",
        "navigationStyle": "custom",
        "disableScroll": true
      }
    },
    {
      "path": "pages/my/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "white",
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/login/login",
      "style": {
        "navigationBarTitleText": "登录"
      }
    },
    {
      "path": "pages/hot/hot",
      "style": {
        "navigationBarTitleText": "热门推荐"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  // 设置 TabBar - 使用自定义 TabBar
  "tabBar": {
    "custom": true,
    "color": "#333",
    "selectedColor": "#863FCE",
    "backgroundColor": "#ffffff",
    "borderStyle": "white",
    "list": [
      {
        "text": "首页",
        "pagePath": "pages/index/index",
        "iconPath": "static/tabs/indexPage.png",
        "selectedIconPath": "static/tabs/indexPageActive.png"
      },
      {
        "text": "获客",
        "pagePath": "pages/customers/index",
        "iconPath": "static/tabs/customer.png",
        "selectedIconPath": "static/tabs/customerActive.png"
      },
      {
        "text": "卖房",
        "pagePath": "pages/house/index",
        "iconPath": "static/tabs/sellHouse.png",
        "selectedIconPath": "static/tabs/sellHuseActive.png"
      },
      {
        "text": "资讯",
        "pagePath": "pages/consult/index",
        "iconPath": "static/tabs/news.png",
        "selectedIconPath": "static/tabs/newsActive.png"
      },
      {
        "text": "我的",
        "pagePath": "pages/my/index",
        "iconPath": "static/tabs/my.png",
        "selectedIconPath": "static/tabs/myActive.png"
      }
    ]
  },
